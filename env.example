# NEXUS-AI Environment Variables
# Copy this file to .env and fill in your actual API keys

# API Keys for Threat Intelligence Services
# Get these from the respective service providers

# VirusTotal API Key (https://www.virustotal.com/gui/join-us)
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Shodan API Key (https://account.shodan.io/register)
SHODAN_API_KEY=your_shodan_api_key_here

# DeepSeek API Key (https://platform.deepseek.com/)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# AbuseIPDB API Key (https://www.abuseipdb.com/api)
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# AlienVault OTX API Key (https://otx.alienvault.com/api)
OTX_API_KEY=your_otx_api_key_here

# Encryption Key (auto-generated if not set)
# NEXUS_ENCRYPTION_KEY=your_encryption_key_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/nexus_ai.log

# Performance Settings
MAX_CONCURRENT_REQUESTS=10
CACHE_TTL=3600
RATE_LIMIT_ENABLED=true

# Security Settings
MAX_FILE_SIZE_MB=100
ALLOWED_FILE_EXTENSIONS=.xml,.txt,.csv,.json,.yaml,.yml
SECURITY_AUDIT_ENABLED=true

# Development Settings
DEBUG_MODE=false
TEST_MODE=false
MOCK_APIS=false 